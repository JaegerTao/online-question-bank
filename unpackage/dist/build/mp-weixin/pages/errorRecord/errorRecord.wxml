<view><view class="topMsg"><icon-text-item class="errorNum" vue-id="f0460f68-1" title="错题量" icon="cuIcon-edit" num="{{errorNum}}" bind:__l="__l"></icon-text-item><icon-text-item class="percent" vue-id="f0460f68-2" title="错误率" icon="cuIcon-roundclose" num="{{percent+'%'}}" bind:__l="__l"></icon-text-item><icon-text-item class="paperCount" vue-id="f0460f68-3" title="试卷数" icon="cuIcon-read" num="{{pCount}}" bind:__l="__l"></icon-text-item></view><view class="recordList"><block wx:if="{{paperlist.length>0}}"><block><block wx:for="{{paperlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><record-item vue-id="{{'f0460f68-4-'+index}}" testid="{{item.TestID}}" title="{{item.TestName}}" subdate="{{item.SubmitTime}}" qsum="{{item.Scount}}" qcrtnum="{{item.tECount}}" issubmit="{{item.IsSubmit}}" isrecord="{{false}}" bind:__l="__l"></record-item></block></block><block wx:if="{{hasmore&&paperlist.length%12==0}}"><view data-event-opts="{{[['tap',[['getMoreRecord',['$event']]]]]}}" class="checkmore" bindtap="__e">查看更多记录...</view></block></block></block><block wx:else><block><view class="nopaper">暂无错题记录</view></block></block></view></view>