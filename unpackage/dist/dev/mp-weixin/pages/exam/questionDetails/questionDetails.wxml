<view><view class="top light"><view class="circlePercent"><circle-percent vue-id="1" bg="#9ca1ff" percent="{{accuracy}}" bind:__l="__l"></circle-percent></view></view><view><view class="time_score_content"><view class="time_part"><view class="cuIcon-time line-green"></view><view class="time_info">{{spendTime.hour+":"+spendTime.minute+":"+spendTime.second}}</view></view><view class="current_part"><view class="cuIcon-roundcheck line-green"></view><view class="current_info">{{count}}</view></view><view class="score_part"><view class="cuIcon-favor line-green"></view><view class="score_info">{{score}}</view></view></view></view><view class="prompt_content" id="head"><view class="prompt_content_text"><text class="cuIcon-title text-red"></text><text>点击题目查看题目解析</text></view><view class="prompt_content_info"><text class="cuIcon-square bg-green "></text>正确<text class="cuIcon-square bg-red"></text>错误</view></view><view class=" scroll_content"><scroll-view class="page padding" style="{{'height:'+(scrollHeight)+';'}}" scroll-y="true"><view class="grid col-5 "><block wx:for="{{questionData}}" wx:for-item="items" wx:for-index="index" wx:key="index"><view class="margin-tb-sm text-center"><button data-event-opts="{{[['tap',[['AppointedSubject',[index]]]]]}}" class="{{['cu-btn round',questionData[index].userAnswer===questionData[index].answer?'bg-green':'bg-red']}}" bindtap="__e">{{index+1}}</button></view></block></view></scroll-view></view><view class="return_content" id="foot"><button class="cu-btn round bg-green btns" type="primary" data-event-opts="{{[['tap',[['returnIndexPage',['$event']]]]]}}" bindtap="__e">返回首页</button></view></view>