<view class="mainframe"><block wx:if="{{!isLogin}}"><view class="notLogin"><bw-swiper class="bwSwiper" vue-id="1" swiperList="{{swiperList}}" swiperType="true" autoplay="true" interval="3000" w_h="{{screenRatio}}" indicatorDots="{{false}}" bind:__l="__l"></bw-swiper><view class="content"><text class="text-shadow text-lg padding solid-bottom">当前未登录,点击按钮登录</text></view><button style="width:50%;" type="primary" data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" bindtap="__e"><text class="cuIcon-pullup"></text><text>开始使用</text></button></view></block><block wx:if="{{isLogin}}"><view class="hasLogin"><view class="top light bg-blue"><view class="circlePercent"><circle-percent vue-id="2" bg="#9ca1ff" percent="{{accuracy}}" bind:__l="__l"></circle-percent></view></view><button type="primary" data-event-opts="{{[['tap',[['goTest',['$event']]]]]}}" bindtap="__e">点击考试</button></view></block></view>